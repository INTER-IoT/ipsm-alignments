        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_entry_vehicle_event">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:CTX a pt:entryVehicles ;
                    pt:hasArray var:array .
                    var:array a pt:Array ;
                    pt:hasElement var:elem .
                    var:elem a pt:Element ;
                    pt:hasaccessId var:accessid ;
                    pt:hascontainerNum1 var:contnum1 ;
                    pt:hascontainerNum2 var:contnum2 ;
                    pt:hascontainerReliability1 var:contrel1 ;
                    pt:hascontainerReliability2 var:contrel2 ;
                    pt:hasplate var:plate ;
                    pt:hasreliability var:rel ;
                    pt:hasdate var:date ;
                    pt:haslane var:lane .
                </align:entity1>
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:truck a sosa:FeatureOfInterest, logico:Truck ;
                    logiserv:involves var:CTX ;
                    iiotex:hasLocalId var:plate .
                    var:CTX a logiserv:Arrive ;
                        iiotex:hasLocalId var:accessid ;
                        logiserv:hasTime [
                            a logico:Instant, time:Instant ;
                                time:inXSDateTimeStamp var:date ;
                        ] ;
                        vp:hasLane var:lane .
                    [ a vp:EntryVehicle, sosa:Observation ;
                        sosa:hasFeatureOfInterest var:truck ;
                        sosa:madeBySensor var:sensor ;
                        sosa:hasResult [
                            a sosa:Result, vp:PlateReading ;
                            iiot:hasResultValue var:plate ;
                            vp:hasReliability var:rel ;
                        ] , [
                        a sosa:Result, vp:Container1Reading ;
                        iiot:hasResultValue var:contnum1 ;
                        vp:hasReliability var:contrel1 ;
                        ] , [
                        a sosa:Result, vp:Container2Reading ;
                        iiot:hasResultValue var:contnum2 ;
                        vp:hasReliability var:contrel2 ;
                        ] ;
                        sosa:resultTime var:date
                    ] .
                    var:sensor a vp:EntrySensor, vp:GateSensor .
                </align:entity2>
                <align:relation>=</align:relation>
                <sripas:transformation rdf:parseType="Literal">
                    <function about="REPLACE">
                        <param order="1" about="&sripas;node_plate"/>
                        <param order="2" val=" "/>
                        <param order="3" val="_"/>
                        <return about="&sripas;node_plate_r"/>
                    </function>
                    <function about="CONCAT">
                        <param order="1" val="http://www.inter-iot.eu/wso2port/entry/truck/"/>
                        <param order="2" about="&sripas;node_plate_r"/>
                        <return about="&sripas;node_truckId"/>
                    </function>
                    <function about="IRI">
                        <param order="1" about="&sripas;node_truckId"/>
                        <return about="&sripas;node_truck"/>
                    </function>
                    <function about="STR">
                        <param order="1" about="&sripas;node_lane"/>
                        <return about="&sripas;node_lane_str"/>
                    </function>
                    <function about="CONCAT">
                        <param order="1" val="http://www.inter-iot.eu/wso2port/gate/"/>
                        <param order="2" about="&sripas;node_lane_str"/>
                        <return about="&sripas;node_laneId"/>
                    </function>
                    <function about="IRI">
                        <param order="1" about="&sripas;node_laneId"/>
                        <return about="&sripas;node_sensor"/>
                    </function>
                </sripas:transformation>
                <sripas:typings rdf:parseType="Literal">
                    <sripas:typing about="&sripas;node_date" datatype="&xsd;dateTimeStamp"/>
                </sripas:typings>
            </align:Cell>
        </align:map>


        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_exit_vehicle_event">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:CTX a pt:exitVehicles ;
                    pt:hasArray var:array .
                    var:array a pt:Array ;
                    pt:hasElement var:elem .
                    var:elem a pt:Element ;
                    pt:hasaccessId var:accessid ;
                    pt:haslane var:lane .
                </align:entity1>
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:CTX a logiserv:Depart ;
                    vp:hasLane var:lane ;
                    iiotex:hasLocalId var:accessid .
                    [
                        a vp:ExitVehicle, sosa:Observation ;
                        sosa:madeBySensor var:sensor
                    ] .
                    var:sensor a vp:ExitSensor, vp:GateSensor .
                </align:entity2>
                <align:relation>=</align:relation>
                <sripas:transformation rdf:parseType="Literal">
                    <function about="STR">
                        <param order="1" about="&sripas;node_lane"/>
                        <return about="&sripas;node_lane_str"/>
                    </function>
                    <function about="CONCAT">
                        <param order="1" val="http://www.inter-iot.eu/wso2port/gate/"/>
                        <param order="2" about="&sripas;node_lane_str"/>
                        <return about="&sripas;node_laneId"/>
                    </function>
                    <function about="IRI">
                        <param order="1" about="&sripas;node_laneId"/>
                        <return about="&sripas;node_sensor"/>
                    </function>
                </sripas:transformation>
            </align:Cell>
        </align:map>

        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_exit_vehicle_event_lpr">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:Obs a sosa:Observation .
                    var:CTZ a logiserv:Depart .
                    var:CTX pt:haslprOk var:lprOk ;
                    pt:haslprPlate var:plate1 ;
                    pt:haslprReliability var:plate_rel1 ;
                    pt:haslprDate var:plate_date1 .
                </align:entity1>
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:truck a sosa:FeatureOfInterest, logico:Truck ;
                    iiotex:hasLocalId var:plate1 ;
                    logiserv:involves var:CTZ .
                    var:CTZ a logiserv:Depart .
                    var:Obs a sosa:Observation ;
                    sosa:hasFeatureOfInterest var:truck ;
                    sosa:hasResult [
                    a sosa:Result, vp:PlateReading ;
                    iiot:hasResultValue var:plate1 ;
                    vp:hasReliability var:plate_rel1 ;
                    vp:resultTime var:plate_date1 ;
                    vp:hasOk var:lprOk ;
                    vp:hasReadingNumber 1;
                    vp:hasSystem "Lpr"
                    ] .
                </align:entity2>
                <align:relation>=</align:relation>
                <sripas:transformation rdf:parseType="Literal">
                    <function about="CONCAT">
                        <param order="1" val="http://www.inter-iot.eu/wso2port/exit/truck/"/>
                        <param order="2" about="&sripas;node_plate1"/>
                        <return about="&sripas;node_truckId"/>
                    </function>
                    <function about="IRI">
                        <param order="1" about="&sripas;node_truckId"/>
                        <return about="&sripas;node_truck"/>
                    </function>
                </sripas:transformation>
            </align:Cell>
        </align:map>

        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_exit_vehicle_event_lprAlarm">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
