                    iiot:hasResultValue var:contnum2 ;
                    vp:hasReliability var:contrel2 ;
                    vp:resultTime var:ocrDate ;
                    vp:hasOk var:ocrOk ;
                    vp:hasReadingNumber 1;
                    vp:hasSystem "ocr"
                    ] .
                </align:entity2>
                <align:relation>=</align:relation>
            </align:Cell>
        </align:map>

        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_exit_vehicle_event_ocrAlarm">
