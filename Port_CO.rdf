                    </function>
                </sripas:transformation>
            </align:Cell>
        </align:map>

        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_exit_vehicle_event_ocr">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:Obs a sosa:Observation .
                    var:CTX pt:hasocrOk var:ocrOk ;
                    pt:hasocrDate var:ocrDate ;
                    pt:hasocrContainerNum1 var:contnum1 ;
                    pt:hasocrContainerNum2 var:contnum2 ;
                    pt:hasocrContainerReliability1 var:contrel1 ;
                    pt:hasocrContainerReliability2 var:contrel2 .
                </align:entity1>
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:Obs a sosa:Observation ;
                    sosa:hasResult  [
                    a sosa:Result, vp:Container1Reading ;
