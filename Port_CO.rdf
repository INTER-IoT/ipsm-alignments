    <align:Alignment>

        <dcelem:title>Port_CO</dcelem:title>
        <exmo:version>2.5</exmo:version>
        <dcelem:creator>SRIPAS</dcelem:creator>
        <dcelem:description>Alignment between port data and GIoTPex ontology. Change of ssn:Deployment to sosa:Platform and ssn:inDeployment to sosa:isHostedBy.</dcelem:description>

        <align:xml>yes</align:xml>
        <align:level>2IPSM</align:level>
