                    pt:hasocrAlarmDate var:ocrAlarmDate ;
                    pt:hasocrAlarmContainerNum1 var:contnum3 ;
                    pt:hasocrAlarmContainerNum2 var:contnum4 ;
                    pt:hasocrAlarmContainerReliability1 var:contrel3 ;
                    pt:hasocrAlarmContainerReliability2 var:contrel4 ;
                </align:entity1>
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
