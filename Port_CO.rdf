        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_exit_vehicle_event_spire">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:CTX a logiserv:Depart .
                    var:CTY  pt:hasspire var:spire .
                </align:entity1>
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:CTX a logiserv:Depart ;
                    vp:hasSpire [
                    a logico:Instant, time:Instant ;
                    time:inXSDateTimeStamp var:spire
                    ] .
                </align:entity2>
