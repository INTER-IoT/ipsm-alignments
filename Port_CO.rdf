        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_meteo_stations">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    var:CTX a pt:meteoStations ;
                    pt:hasArray var:array .
                    var:array a pt:Array ;
                    pt:hasElement var:elem .
                    var:elem a pt:Element ;
                    pt:hasactive var:active ;
                    pt:haslatitude var:lat ;
                    pt:haslocation var:loc ;
                    pt:haslongitude var:long ;
                    pt:hasmeteoStationId var:id ;
                    pt:hasname var:name ;
                    pt:hasportId var:portid .
