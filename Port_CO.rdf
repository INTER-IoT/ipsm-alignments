                    ] .
                </align:entity2>
                <align:relation>=</align:relation>
                <sripas:transformation rdf:parseType="Literal">
                    <function about="CONCAT">
                        <param order="1" val="http://www.inter-iot.eu/wso2port/exit/truck/"/>
                        <param order="2" about="&sripas;node_plate2"/>
